print('B'+' '+' '+' '+'B')
print('B'+' '+' '+' '+'B')
print('B'+'B'+'B'+'B')
print('B'+' '+' '+' '+'B')
print('B'+' '+' '+' '+'B')
print('B'+'B'+'B'+'B')
print(' '+' '+'A'+' '+' ')
print(' '+'A'+' '+'A'+' ')
