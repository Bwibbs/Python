weight = 4.8

#ground shipping
if weight <=2:
  cost_ground = weight * 1.5 +20
elif weight <= 6:
   cost_ground = weight * 3 +20
elif weight <= 10:
  cost_ground = weight * 4 +20
elif weight >= 10:
  cost_ground = weight * 4.75 +20
else:
  print('Error')
print('Cost of ground shipping is $',cost_ground) #print cost_ground

#ground shipping premium

premium_ground_shipping_flat_charge = 125

print('Ground Shipping Premium $',premium_ground_shipping_flat_charge )

#drone shipping
if weight <=2:
  cost_drone = weight * 4.5
elif weight <= 6:
   cost_drone = weight * 9
elif weight <= 10:
  cost_drone = weight * 12
elif weight >= 10:
  cost_drone = weight * 14
else:
  print('Error')
print('Cost of drone shipping is $', cost_drone) #print cost_drone
